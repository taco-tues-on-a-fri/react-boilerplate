{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Taco-Tues-on-a-Fri's Special Boilerplate",
  "main": "src/App.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV='production' webpack",
    "build-for-windows": "SET NODE_ENV='production' && webpack"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "syntax-dynamic-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taco-tues-on-a-fri/react-boilerplate"
  },
  "keywords": [
    "React",
    "Boilerplate"
  ],
  "author": "taco-tues-on-a-fri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taco-tues-on-a-fri/react-boilerplate/issues"
  },
  "homepage": "https://github.com/taco-tues-on-a-fri/react-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "prop-types": "^15.7.2",
    "react-dev-utils": "^10.2.1",
    "serialize-javascript": "^3.1.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
